pipeline {
    agent any

    stages {
        stage('DEV from GitHub') {
            steps {
                echo 'Hello from DEV environment'
            }
        }
        stage('UAT from GitHub') {
            steps {
                echo 'Hello from UAT environment'
            }
        }
        stage('PROD from GitHub') {
            steps {
                echo 'Hello from PROD environment'
            }
        }
    }
}
